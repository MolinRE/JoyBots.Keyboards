<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <AssemblyName>JoyBots.Keyboards</AssemblyName>
        <RootNamespace>JoyBots.Keyboards</RootNamespace>
        <PackageVersion>0.0.2</PackageVersion>
        <Title>JoyBots.Keyboards</Title>
        <Authors>Konstantin Komarov</Authors>
        <Description>Keyboards for Telegram bots.</Description>
        <Copyright>Konstantin Komarov, 2024</Copyright>
        <PackageProjectUrl>https://github.com/MolinRE/JoyBots.Keyboards</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/MolinRE/JoyBots.Keyboards/blob/main/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/MolinRE/JoyBots.Keyboards</RepositoryUrl>
        <RepositoryType>GitHub</RepositoryType>
        <PackageTags>telegram</PackageTags>
        <AssemblyVersion>0.0.2</AssemblyVersion>
        <FileVersion>0.0.2</FileVersion>
        <NeutralLanguage>en</NeutralLanguage>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Telegram.Bot" Version="17.0.0" />
    </ItemGroup>

</Project>
